{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    { "src": "/(.*\\.css|.*\\.js|.*\\.png|.*\\.jpg|.*\\.svg)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/app.py" }
  ]
}
